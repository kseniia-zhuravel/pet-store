<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Store Test" parallel="methods">
    <test name="Order validation">
        <classes>
            <class name="store.OrderValidationTest"/>
        </classes>
    </test>
    <test name="Order details">
